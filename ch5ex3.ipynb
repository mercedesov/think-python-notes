    "def ackerman(m, n):\n",
    "    if m == 0:\n",
    "        return n + 1\n",
    "    elif m > 0 and n == 0:\n",
    "        return ackerman(m - 1, 1)\n",
    "    elif m > 0 and n > 0:\n",
    "        return ackerman(m - 1, ackerman(m, n - 1))"
   ]
