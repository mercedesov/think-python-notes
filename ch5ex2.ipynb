   "source": [
    "def hypot(leg1,leg2):\n",
    "    '''returns the length of\n",
    "       the hypotenuse of a right\n",
    "       triangle given the lengths of\n",
    "       the other two legs as arguments'''\n",
    "    hyp = leg1**2 + leg2**2\n",
    "    return hyp ** 0.5"
   ]
