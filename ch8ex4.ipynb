    "import re\n",
    "\n",
    "with open(\"montecristo.txt\", \"r\") as f:\n",
    "    counter = 0\n",
    "    pattern = re.compile(r'pale.*') \n",
    "    for line in f:\n",
    "        if pattern.search(line):\n",
    "            counter += 1\n",
    "\n",
    "print(counter)"
   ]
